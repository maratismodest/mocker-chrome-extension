{
  "manifest_version": 3,
  "name": "Mock Data Extension",
  "version": "1.0",
  "description": "Returns mock data for specific routes",
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "storage-manager.html"
  }
}